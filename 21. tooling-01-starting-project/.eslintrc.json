{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
       
        "quotes": [ "error", "single", "double" ],
        "camelcase":"error",
        "semi":"error",
        "no-undef": "off"
    }
}
